# See http://editorconfig.org/

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[{*.php,*.html}]
indent_style = space
indent_size = 4

[{*.js,*.cjs}]
indent_style = space
indent_size = 2

[{*.css,*.scss}]
indent_style = space
indent_size = 4

